{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "linuxAdminUsername": {
            "value": "azureuser"
        },
        "agentCount": {
            "value": 1
        },
        "dnsNamePrefix": {
            "value": "ascswarmcluster"
        },
        "agentVMSize": {
            "value": "Standard_DS2"
        },
        "masterCount": {
            "value": 1
        },
        "orchestratorType": {
            "value": "Swarm"
        },
        "sshRSAPublicKey": {
            "value": "ssh-rsa "
        },
        "enableDiagnostics": {
            "value": true
        }
    }
}